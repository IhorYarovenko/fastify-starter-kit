{
    "env": {
        "node": true,
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "indent": [
            2,
            2,
            {
                "SwitchCase": 1
            }
        ],
        "import/extensions": "off",
        "no-underscore-dangle": 0,
        "no-plusplus": 0,
        "no-await-in-loop": 0,
        "no-console": 2,
        "consistent-return": 0,
        "class-methods-use-this": 0,
        "no-param-reassign": 0,
        "no-shadow": 0,
        "func-names": 0,
        "no-mixed-operators": 0,
        "no-throw-literal": 0,
        "no-prototype-builtins": 0,
        "no-restricted-syntax": 0
    }
}
